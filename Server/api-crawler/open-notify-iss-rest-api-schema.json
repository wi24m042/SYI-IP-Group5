{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "message": {
      "type": "string"
    },
    "timestamp": {
      "type": "number"
    },
    "iss_position": {
      "type": "object",
      "properties": {
        "latitude": {
          "type": "string"
        },
        "longitude": {
          "type": "string"
        }
      },
      "required": ["latitude", "longitude"]
    }
  },
  "required": ["message", "timestamp", "iss_position"]
}
